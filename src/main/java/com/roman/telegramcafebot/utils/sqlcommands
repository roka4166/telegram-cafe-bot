create table coworker_info(
    id int generated by default as identity primary key,
    chat_id int not null,
    is_active boolean not null
);

create table order_info(
    id int generated by default as identity primary key,
    chat_id int not null,
    total_price int not null,
    items varchar(500) not null
);

insert into button (name, belongs_to_menu, callbackdata) VALUES ('–î–∞', 'bookingconfirmationmenu', 'CONFIRMBOOKING'),
                                                                ('–ù–µ—Ç', 'bookingconfirmationmenu', 'START');

insert into button (name, belongs_to_menu, callbackdata) VALUES ('–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å –±—Ä–æ–Ω—å', 'bookingconfirmationadminmenu', 'RESERVATION_CONFIRMED'),
                                                                ('–û—Ç–∫–ª–æ–Ω–∏—Ç—å –±—Ä–æ–Ω—å', 'bookingconfirmationadminmenu', 'RESERVATION_DECLINED');

create table reservation(
    id int generated by default as identity primary key,
    chat_id bigint not null,
    tablenumber int not null,
    name varchar(50) not null,
    time varchar(50) not null,
    confirmed boolean,
    comment varchar(500)
)